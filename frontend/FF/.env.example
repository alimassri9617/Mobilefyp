# Environment Configuration Template for UniAssist Mobile
# Copy this file to .env and fill in your actual values

# API Configuration
API_BASE_URL=http://your-backend-url:5000/api
API_TIMEOUT=10000

# Push Notifications
EXPO_PROJECT_ID=your-expo-project-id-here
PUSH_NOTIFICATIONS_ENABLED=true

# App Configuration
APP_NAME=UniAssist Mobile
APP_VERSION=1.0.0
DEBUG_MODE=false

# Socket Configuration
SOCKET_URL=http://your-backend-url:5000
SOCKET_TIMEOUT=5000

# Storage Configuration
STORAGE_PREFIX=uniassist_mobile_

# Feature Flags
ENABLE_POMODORO=true
ENABLE_LOST_AND_FOUND=true
ENABLE_NOTES=true
ENABLE_APPOINTMENTS=true
ENABLE_CHATBOT=true

# Notification Settings
DEFAULT_NOTIFICATION_SOUND=true
DEFAULT_VIBRATION=true
APPOINTMENT_REMINDER_MINUTES=15

# Development Settings
LOG_LEVEL=info
ENABLE_REDUX_DEVTOOLS=false

